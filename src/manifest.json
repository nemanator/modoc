{
	"manifest_version": 2,
	"name": "Modoc",
	"version": "0.1",
	"permissions": [
      "https://registry.npmjs.org/*"
  ],
	"content_scripts": [
		{
			"matches": ["*://*.github.com/*"],
			"css": ["main.bundle.css"],
			"js": ["index.bundle.js"]
		}
	],
	"icons": {
		"16": "document-16.png",
		"48": "document-48.png",
		"128": "document-128.png"
	}
}